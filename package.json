{
  "name": "@cloudbolt/cb-api-helper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js .",
    "precommit": "npm run lint",
    "prepare": "husky install",
    "test": "jest --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudBoltSoftware/cb-api-helper.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "axios": "^0.25.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint",
      "git add"
    ]
  }
}
