{
  "name": "@cloudbolt/cb-api-helper",
  "version": "0.1.0",
  "scripts": {
    "co:login": "aws codeartifact login --tool npm --repository cloudbolt-npm --domain cloudbolt --domain-owner 499620025628",
    "husky:install": "husky install",
    "build": "webpack",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js .",
    "precommit": "npm run lint",
    "prepare": "concurrently \"npm run husky:install\" \"npm run co:login\"",
    "test": "jest --silent"
  },
  "main": "dist/cb-api-helper.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudBoltSoftware/cb-api-helper.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "axios": "^0.25.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint",
      "git add"
    ]
  }
}
